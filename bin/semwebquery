#!/bin/bash
# $Id: semwebquery,v 1.1 2006/10/09 12:27:22 cyganiak Exp $
NG4J_ROOT=${0%/*}/..
SEP=':'
if [ "$(uname)" = "Cygwin" ]; then SEP=';'; fi
CP=$NG4J_ROOT/build
for jar in "$NG4J_ROOT"/lib/*.jar
do
  if [ ! -e "$jar" ]; then continue; fi
  CP="$CP$SEP$jar"
done
exec java -cp "$CP" semweb.query "$@"
